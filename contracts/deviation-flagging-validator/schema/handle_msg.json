{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "set_flagging_threshold"
      ],
      "properties": {
        "set_flagging_threshold": {
          "type": "object",
          "required": [
            "threshold"
          ],
          "properties": {
            "threshold": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "set_flags_address"
      ],
      "properties": {
        "set_flags_address": {
          "type": "object",
          "required": [
            "flags"
          ],
          "properties": {
            "flags": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "validate"
      ],
      "properties": {
        "validate": {
          "type": "object",
          "required": [
            "answer",
            "previous_answer",
            "previous_round_id",
            "round_id"
          ],
          "properties": {
            "answer": {
              "$ref": "#/definitions/Uint128"
            },
            "previous_answer": {
              "$ref": "#/definitions/Uint128"
            },
            "previous_round_id": {
              "$ref": "#/definitions/Uint128"
            },
            "round_id": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}

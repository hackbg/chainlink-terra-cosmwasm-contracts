{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Requester",
  "type": "object",
  "required": [
    "authorized",
    "delay",
    "last_started_round"
  ],
  "properties": {
    "authorized": {
      "type": "boolean"
    },
    "delay": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "last_started_round": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
